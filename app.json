{
  "expo": {
    "name": "RickAndMorty",
    "slug": "RickAndMorty",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/rick-and-morty.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.anonymous.RickAndMorty"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/rick-and-morty.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.anonymous.RickAndMorty"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/rick-and-morty.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/rick-and-morty.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff",
          "dark": {
            "backgroundColor": "#000000"
          }
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "ba6cfc4c-e6e1-4cc1-a13b-ed1daa0794e5"
      }
    }
  }
}
